{
  "name": "zapi",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.9",
    "@types/prettier": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "build": "tsc",
    "play": "nodemon -e ts --exec 'ts-node' src/index.ts",
    "record": "ts-node src/examples/record.ts",
    "autopilot": "ts-node src/autopilot.ts",
    "autopilot-repeat": "ts-node src/autopilot-repeat.ts",
    "schedule": "ts-node src/examples/export-schedule.ts",
    "sync": "nodemon -e ts --exec 'ts-node' 'src/examples/sync-welocal.ts'",
    "dev": "nodemon -e ts --exec 'ts-node' src/dev.ts",
    "dev:autopilot": "nodemon -e ts --exec 'ts-node' src/autopilot.ts",
    "dev:autopilot-repeat": "nodemon -e ts --exec 'ts-node' src/autopilot-repeat.ts",
    "start:autopilot": "node dist/autopilot",
    "start:autopilot-repeat": "node dist/autopilot-repeat",
    "test": "npx jest",
    "test-coverage": "npx jest --coverage",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,json,md}\""
  },
  "dependencies": {
    "@types/node-osc": "^6.0.3",
    "axios": "^1.13.1",
    "basic-ftp": "^5.0.5",
    "dotenv": "^16.4.5",
    "luxon": "^3.4.4",
    "node-osc": "^9.1.4",
    "node-xlsx": "^0.24.0",
    "slugify": "^1.6.6"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
